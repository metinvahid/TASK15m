@model Country


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />
</head>
<body>
    <form method="post">
        <div class="mb-3">
            <label asp-for="Name" class="form-label">Name</label>
            <input class="form-control" asp-for="Name" aria-describedby="nameHelp">
            <div id="nameHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>

        <div class="mb-3">
            <label asp-for="Code" class="form-label">Code</label>
            <input class="form-control" asp-for="Code" aria-describedby="codeHelp">
            <div id="codeHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>

        <div class="mb-3">
            <label asp-for="Id" class="form-label">Id</label>
            <input class="form-control" asp-for="Id" aria-describedby="IdHelp">
            <div id="idHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>
       

        <button type="submit" class="btn btn-primary">Yadda saxla</button>
    </form>

</body>
</html>
